            <artifactId>features-bgp</artifactId>
            <classifier>features</classifier>
            <version>${bgpcep.version}</version>
            <type>xml</type>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>org.opendaylight.bier</groupId>
            <artifactId>odl-bier-std-driver</artifactId>
            <version>${project.version}</version>
            <type>xml</type>
            <classifier>features</classifier>
        </dependency>
        <dependency>
            <groupId>org.opendaylight.bier</groupId>
            <artifactId>bierman-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.opendaylight.bier</groupId>
            <artifactId>bierman-impl</artifactId>
            <version>${project.version}</version>
        </dependency>
    </dependencies>
</project>
